{
	"name": "@tolo/app",
	"version": "1.4.4",
	"private": true,
	"main": "src/index.ts",
	"scripts": {
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"web:build": "bash -c 'echo \"üöÄ Starting web build with 2-minute timeout...\"; expo export --platform web & pid=$!; (sleep 120 && echo \"‚ùå Build timed out after 2 minutes\" && kill $pid 2>/dev/null && exit 0) & timeout_pid=$!; if wait $pid; then kill $timeout_pid 2>/dev/null; if [ -d dist ]; then echo \"‚úÖ Web build completed successfully\"; else echo \"‚ùå Build failed - dist directory not found\" && exit 1; fi; else kill $timeout_pid 2>/dev/null && echo \"‚ùå Build failed with exit code $?\" && exit 1; fi'",
		"test": "jest --watchAll=false",
		"test:watch": "jest --watchAll",
		"test:coverage": "jest --coverage --watchAll=false",
		"lint": "oxlint --type-aware",
		"lingui:extract": "lingui extract --clean",
		"codegen": "wrangler types && oxfmt ./worker-configuration.d.ts",
		"format": "oxfmt .",
		"format:check": "oxfmt --check .",
		"typecheck": "tsgo --noEmit",
		"deploy:workers": "wrangler deploy --upload-source-maps --var SENTRY_RELEASE:$(sentry-cli releases propose-version)",
		"deploy": "wrangler deploy --outdir dist --upload-source-maps --var SENTRY_RELEASE:$(sentry-cli releases propose-version)",
		"update:publish": "eas update --auto",
		"clean": "rm -rf node_modules ios dist .wrangler coverage android .expo && bun install",
		"sentry:sourcemaps": "_SENTRY_RELEASE=$(sentry-cli releases propose-version) && sentry-cli releases new $_SENTRY_RELEASE --org=tolo-cafe --project=workers && sentry-cli sourcemaps upload --org=tolo-cafe --project=workers --release=$_SENTRY_RELEASE --strip-prefix 'dist/..' dist",
		"postdeploy": "bun run sentry:sourcemaps",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"prepare": "husky"
	},
