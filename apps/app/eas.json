{
	"build": {
		"common": {
			"bun": "1.3.9",
			"node": "22.22.0",
			"env": {
				"NODE_OPTIONS": "--max-old-space-size=4096"
			}
		},
		"development": {
			"channel": "development",
			"developmentClient": true,
			"distribution": "internal",
			"extends": "common"
		},
		"preview": {
			"channel": "preview",
			"distribution": "internal",
			"extends": "common"
		},
		"production": {
			"autoIncrement": true,
			"channel": "production",
			"extends": "common"
		}
	},
	"cli": {
		"appVersionSource": "remote",
		"version": ">= 16.19.3"
	},
	"submit": {
		"production": {
			"ios": {
				"appleTeamId": "AUR7UR6M72",
				"ascAppId": "6749597635"
			}
		}
	}
